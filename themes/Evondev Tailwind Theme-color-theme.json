    "gitDecoration.ignoredResourceForeground": "#6272a4",
    "gitDecoration.modifiedResourceForeground": "#8be9fd",
    "gitDecoration.untrackedResourceForeground": "#20E3B2",
    "titleBar.activeBackground": "#1e1f35",
    "editorSuggestWidget.background": "#21222c",
    "editorSuggestWidget.border": "#17223f",
    "editorSuggestWidget.selectedBackground": "#ffffff18",
    "selection.background": "#ffffff18",
    "list.focusBackground": "#44475a75",
    "list.hoverBackground": "#44475a75",
    "list.inactiveSelectionBackground": "#44475a75",
    "editor.background": "#191a2e",
