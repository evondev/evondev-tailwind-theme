    "sideBar.foreground": "#f8fafc",
    "quickInput.foreground": "#f8fafc",
    "titleBar.activeForeground": "#f8fafc",
    "input.foreground": "#f8fafc",
    "editorGroup.border": "#21293d",
    "editorGroupHeader.border": "#21293d"
